my_fake_list = [
    'Preston',
    22,
    'Hello!',
    'Good Morning!'
]

print(my_fake_list)
# ['Preston', 22, 'Hello!', 'Good Morning!']

# printing values of list
print(my_fake_list[0]) # first one  - 'Preston'
print(my_fake_list[1]) # second one - 22




